{
  "name": "abacus-contracts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ansiToHtml": "npx ts-node ./scripts/ansiToHtml.ts",
    "cs": "npx ts-node ./scripts/compile/compileSingleContract.ts",
    "build": "./build.sh",
    "deploy": "npx ts-node runWithoutWarnings.ts env DEBUG=1 npx ts-node scripts/deploy.ts",
    "benchmark": "npx ts-node runWithoutWarnings.ts npx ts-node scripts/benchmarkPlayground.ts",
    "localnode": "./substrate-contracts-node --dev --tmp -lerror,runtime::contracts=debug --ws-max-connections 5000 --pruning=archive",
    "test:debug": "npx ts-node runWithoutWarnings.ts env DEBUG=1 npx ts-node scripts/runScenarioWithDebug.ts",
    "test": "./runTests.sh",
    "generateTypes": "npx ts-node ./runWithoutWarnings.ts npx typechain-polkadot --in artifacts --out typechain && npx ts-node ./scripts/typechain/replaceQueryCalls.ts && npx ts-node ./scripts/typechain/replaceHandleReturnType.ts && npx ts-node ./scripts/typechain/generateEventsEnum.ts",
    "codeDuplication": "npx jscpd --min-lines 5 --min-tokens 50 --pattern './!(target)/**/*.rs' --reporters html -o jscpd",
    "clippy": "script -efq ./clippy.log -c 'env CARGO_TERM_COLOR=always cargo clippy -- -W clippy::pedantic'",
    "lint": "yarn lint:format && yarn lint:fix",
    "lint:fix": "eslint . --ext .js,.ts --fix --ignore-path ./.gitignore",
    "lint:format": "pretty-quick  --loglevel warn --write \"./**/*.{js,ts,md,json}\" ",
    "prepare": "husky install"
  },
  "dependencies": {
    "@727-ventures/typechain-polkadot": "^0.6.7",
    "@727-ventures/typechain-types": "^0.0.21",
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/register": "^7.17.7",
    "@polkadot/api": "^9.10.3",
    "@polkadot/api-contract": "^9.10.3",
    "@polkadot/keyring": "^10.1.11",
    "@polkadot/util": "^10.1.11",
    "@polkadot/util-crypto": "^10.1.11",
    "@types/chroma-js": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "ansi-to-html": "^0.7.2",
    "bn.js": "^5.2.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-formatter-monkeypatch": "^1.0.1",
    "chart.js": "^3.9.1",
    "chartjs-node-canvas": "^4.1.6",
    "chroma-js": "^2.4.2",
    "color-name-list": "^9.21.0",
    "commander": "^5.1.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "jscpd": "^3.5.1",
    "json2csv": "^5.0.7",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.0",
    "mocha": "^10.2.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/json2csv": "^5.0.3",
    "@types/lodash": "^4.14.185",
    "@types/mocha": "^9.1.1",
    "husky": "^8.0.0",
    "tsconfig-paths": "^4.1.0"
  },
  "packageManager": "yarn@3.3.0"
}
